<p id="notice"><%= notice %></p>

<h4 class="widget-title">Draw Output Groups</h4>

<table>
  <thead>
    <tr>
      <th>Size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @draw_output_groups.each do |draw_output_group| %>
      <tr>
        <td><%= draw_output_group.size %></td>
        <td><%= link_to 'Show', draw_output_group %></td>
        <td><%= link_to 'Edit', edit_draw_output_group_path(draw_output_group) %></td>
        <td><%= link_to 'Destroy', draw_output_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Draw Output Group', new_draw_output_group_path %>
